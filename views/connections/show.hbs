<div class="container profile-container">
{{#each users }}
<div class="profile-header col-xs-12">
    <div class="profile-pic col-xs-3">
      <img class="profile-img" src={{img_url}} width="250">
    </div>
  <div class="profile-info col-xs-9">
    <h1 class="main-title-profile">{{user_name}}</h1>
    <h3 class="profile-content">{{first_name}} {{last_name}}</h3>
  </div>
  <div class="social-icon col-xs-9">
    <a href={{twitter}} class="btn-social-icon btn-twitter">
      <span class="fa fa-twitter"></span>
    </a>
    <a href={{github}} class="btn-social-icon btn-github">
      <span class="fa fa-github"></span>
    </a>
    <a href={{linkedin}} class="btn-social-icon btn-linkedin">
      <span class="fa fa-linkedin"></span>
    </a>
  </div>
</div>
<div class="profile-body col-xs-12">
  <div class="sidebar col-xs-3">
  </div>
  <div class="profile-bio col-xs-9"><h2>Bio</h2>{{bio}}</div>
  <div class="profile-skills col-xs-9"><h2>Skills</h2>{{skills}}</div>
</div>
  {{/each}}
</div>
<br>
